<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="300dp"
    android:padding="16dp"
    android:background="#226666">

    <TextView
        android:id="@+id/view_ime_korisnik"
        android:layout_width="166dp"
        android:layout_height="44dp"
        android:padding="1dp"
        android:textColor="#000"
        android:text="TextView"
        android:textSize="25sp"
        app:layout_constraintBottom_toTopOf="@+id/view_ime_grad"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/view_ime_parking"
        android:layout_width="167dp"
        android:layout_height="44dp"
        android:textColor="#000"
        android:text="TextView"
        android:textSize="25sp"
        android:padding="1dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/view_ime_grad" />

    <TextView
        android:id="@+id/view_timeslot"
        android:layout_width="167dp"
        android:layout_height="44dp"
        android:padding="1dp"
        android:textColor="#000"
        android:text="TextView"
        android:textSize="25sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/view_ime_parking" />

    <TextView
        android:id="@+id/view_ime_grad"
        android:layout_width="166dp"
        android:layout_height="44dp"
        android:padding="1dp"
        android:textColor="#000"
        android:text="TextView"
        android:textSize="25sp"
        tools:layout_editor_absoluteX="212dp"
        tools:layout_editor_absoluteY="46dp" />


</androidx.constraintlayout.widget.ConstraintLayout>

