<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.lab"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="16"
        android:targetSdkVersion="30" />

    <application
        android:allowBackup="true"
        android:appComponentFactory="androidx.core.app.CoreComponentFactory"
        android:debuggable="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:testOnly="true"
        android:theme="@style/Theme.Lab" >
        <activity android:name="com.example.lab.model.adapters.ParkingAdapter" />
        <activity android:name="com.example.lab.view.activities.ParkingActivity" />
        <activity
            android:name="com.example.lab.view.activities.ReservationActivity"
            android:label="@string/title_activity_reservation"
            android:theme="@style/Theme.Lab.NoActionBar" />
        <activity android:name="com.example.lab.view.activities.MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.lab.view.activities.CitiesActivity" />
        <activity android:name="com.example.lab.view.activities.RegisterActivty" />
    </application>

</manifest>